package com.example.demo.model;

import javax.persistence.Column;

import javax.persistence.Entity;
import javax.persistence.Table;
import javax.persistence.GenerationType;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;
@Entity
@Table(name = "Individual")
public class Individual {

	
	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	private int IN_ID;
	
	@Column(name="IN_NAME")
	private String IN_NAME;
	@Column(name="IN_ADDRESS")
	private String IN_ADDRESS;
	@Column(name="IN_CONTACT")
	private int IN_CONTACT;
	@Column(name="IN_EMAIL")
	private String IN_EMAIL;
	@Column(name="DONATION_TYPE")
	private String DONATION_TYPE;
	@Column(name="AMOUNT")
	private int AMOUNT;
	
	public Individual() {
		
	}
	public Individual( String iN_NAME, String iN_ADDRESS, int iN_CONTACT, String iN_EMAIL,
			String dONATION_TYPE, int aMOUNT) {
		super();
		IN_NAME = iN_NAME;
		IN_ADDRESS = iN_ADDRESS;
		IN_CONTACT = iN_CONTACT;
		IN_EMAIL = iN_EMAIL;
		DONATION_TYPE = dONATION_TYPE;
		AMOUNT = aMOUNT;
	}

	public int getIN_ID() {
		return IN_ID;
	}
	public void setIN_ID(int iN_ID) {
		IN_ID = iN_ID;
	}
	public String getIN_NAME() {
		return IN_NAME;
	}
	public void setIN_NAME(String iN_NAME) {
		IN_NAME = iN_NAME;
	}
	public String getIN_ADDRESS() {
		return IN_ADDRESS;
	}
	public void setIN_ADDRESS(String iN_ADDRESS) {
		IN_ADDRESS = iN_ADDRESS;
	}
	public int getIN_CONTACT() {
		return IN_CONTACT;
	}
	public void setIN_CONTACT(int iN_CONTACT) {
		IN_CONTACT = iN_CONTACT;
	}
	public String getIN_EMAIL() {
		return IN_EMAIL;
	}
	public void setIN_EMAIL(String iN_EMAIL) {
		IN_EMAIL = iN_EMAIL;
	}
	public String getDONATION_TYPE() {
		return DONATION_TYPE;
	}
	public void setDONATION_TYPE(String dONATION_TYPE) {
		DONATION_TYPE = dONATION_TYPE;
	}
	public int getAMOUNT() {
		return AMOUNT;
	}
	public void setAMOUNT(int aMOUNT) {
		AMOUNT = aMOUNT;
	}
	

	}


